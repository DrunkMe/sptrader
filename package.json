{
    "name" : "Sptrader",
    "version" : "0.0.2",
    "author" : "Joseph C Wang <joequant@gmail.com>",
    "license" : "BSD-2-Clause",
    "repository" : {
	"type" : "git",
	"url" : "https://github.com/joequant/sptrader.git"
    },
    "dependencies" : {
	"react" : "15.x",
	"react-dom" : "15.x",
	"react-datagrid" : "latest",
	"react-bootstrap" : "latest",
	"react-addons-linked-state-mixin" : "latest"
    },
    "devDependencies" : {
	"browserify" : "latest",
	"watchify" : "latest",
	"babelify" : "latest",
	"babel-preset-es2015" : "latest",
	"babel-preset-react" : "latest",
	"babel-preset-stage-2" : "latest"
    },
    "scripts": {
	"build-js": "browserify -t [ babelify --presets [ es2015 react stage-2 ] ]  static/sptrader.js -o static/bundle.js",
	"watch-js": "watchify -t  [ babelify --presets [ es2015 react stage-2 ] ] static/sptrader.js -o static/bundle.js"
    }
}
